<h1>Listing seasons</h1>

<table>
  <tr>
    <th>Season name</th>
    <th>Season start</th>
    <th>Season end</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @seasons.each do |season| %>
  <tr>
    <td><%= season.season_name %></td>
    <td><%= season.season_start %></td>
    <td><%= season.season_end %></td>
    <td><%= link_to 'Show', season %></td>
    <td><%= link_to 'Edit', edit_admin_season_path(season) %></td>
    <td><%= link_to 'Destroy', season, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Season', new_admin_season_path %>
